{
  "name": "ts-sequelize-cli",
  "version": "0.2.6",
  "description": "Sequelize CLI for TS",
  "author": "vadymsushkov",
  "url": "https://github.com/vadymsushkovdev/sequelize-cli-types",
  "email": "vadymsushkovdev@gmail.com",
  "keywords": [
    "migrate",
    "migration",
    "migrations",
    "sequelize",
    "database"
  ],
  "main": "dist/src/components/Commands/commands.js",
  "types": "dist/src/components/Commands/commands.d.ts",
  "prepublish": "tsc && npm link",
  "bin": {
    "sqlz": "dist/src/components/Commands/commands.js"
  },
  "scripts": {
    "build": "tsc --skipLibCheck",
    "start": "ts-node ./src/components/Commands/initialization.commands.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "commander": "7.2.0",
    "fs-extra": "10.0.0",
    "mysql2": "latest",
    "path": "0.12.7",
    "pg": "latest",
    "pg-hstore": "latest",
    "sequelize": "6.6.2",
    "ts-node": "10.0.0",
    "umzug": "2.3.0"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.11",
    "@types/umzug": "2.3.0",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "eslint": "7.22.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-airbnb-typescript": "11.0.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-import": "2.22.1",
    "prettier": "2.3.1",
    "typescript": "4.3.2"
  },
  "engines": {
    "node": ">=12.20.0"
  },
  "license": "MIT"
}
